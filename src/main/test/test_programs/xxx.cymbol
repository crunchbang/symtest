void main() 
{
    int x = input();
    int y = 0;
    int c = 0;
    int m = 0;
    while (true){
            m=m+1;
        if ((x>10) && (x<110)) {
            if (x > 50)
            {
                y = x + 2;
            }
            else
            {
                y = x * 2;
            }
            if((y>100) && (y<180))
            {
                x = x + 2;
            }
            else
            {
                if((y>60) && (y<70))
                {
                    x = x + 15;
                }
                else
                {

                    if(y>65)
                    {
                        x = x + 1;
                    }
                    else
                    {
                        x = x + 5;
                    }
                }

            }
            if((x < 50)&&(x>44))
            {

                x = x + 65;
            }
            else
            {

                x = x + 20;
            }
            c=c+1;
        } else {
            m=m+1;
        }
    }
}


